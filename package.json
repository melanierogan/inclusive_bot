{
  "name": "inclusive_bot",
  "version": "1.0.0",
  "description": "Checking for unfriendly terminology in your repo",
  "author": "Melanie Rogan <melanie.rogan@ft.com>",
  "license": "ISC",
  "repository": "https://github.com//inclusive-bot-test.git",
  "homepage": "https://github.com//inclusive-bot-test",
  "bugs": "https://github.com//inclusive-bot-test/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "12.3.3"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.0",
    "nodemon": "^1.19.3",
    "smee-client": "^1.1.0",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": "^10.15.3"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
